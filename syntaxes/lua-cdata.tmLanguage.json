{
  "scopeName": "source.xml",
  "patterns": [
    {
      "begin": "<!\\[CDATA\\[",
      "end": "\\]\\]>",
      "contentName": "source.lua",
      "patterns": [
        {
          "include": "source.lua"
        }
      ]
    },
    {
      "include": "text.xml"
    }
  ],
  "repository": {
    "xml": {
      "patterns": [
        {
          "include": "text.xml"
        }
      ]
    },
    "lua": {
      "patterns": [
        {
          "include": "source.lua"
        }
      ]
    }
  }
}
