{
  "scopeName": "source.xml",
  "patterns": [
    {
      "include": "#xml"
    }
  ],
  "repository": {
    "xml": {
      "patterns": [
        {
          "begin": "<!\\[CDATA\\[",
          "end": "\\]\\]>",
          "contentName": "source.lua",
          "patterns": [
            {
              "include": "source.lua"
            }
          ]
        },
        {
          "include": "text.xml"
        }
      ]
    }
  }
}
