{
  "name": "firecastsupport",
  "displayName": "Firecast Support",
  "description": "Esta extensão visa facilitar e agilizar o desenvolvimento de fichas para o Firecast utilizando o SDK 3, de maneira prática você possuirá snippets que auxiliarão você a escrever todas as tags do SDK 3.",
  "icon": "img/firecast.png",
  "version": "0.0.4",
  "repository": "https://github.com/Krampus-update/Firecast-Extension",
  "scripts": {
    "deploy": "vsce publish",
    "test": "vscode-test"
  },
  "galleryBanner": {
    "color": "#7159c1",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Snippets",
    "Extension Packs"
  ],
  "contributes": {
    "grammars": [
      {
        "language": "xml",
        "scopeName": "source.xml",
        "path": "./syntaxes/xml-lua.tmLanguage.json",
        "embeddedLanguages": {
          "source.lua": "lua",
          "text.xml": "xml"
        }
      }
    ],
    "snippets": [
      {
        "language": "xml",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "publisher": "PedroLuca",
  "devDependencies": {
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^2.21.0"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
